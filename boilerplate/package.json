{
  "name": "tbd",
  "version": "0.0.0",
  "main": "src/index.ts",
  "author": "tbd",
  "private": true,
  "scripts": {
    "dream": "yarn dreamts",
    "dreamjs": "node  --experimental-modules dist/test-app/cli/index.js",
    "dreamts": "npx ts-node --transpile-only -r ./src/helpers/loadEnv.ts test-app/cli/index.ts",
    "build": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version yarn --cwd=node_modules/@rvohealth/dream build && echo \"building your app...\" && rm -rf dist && echo \"node --version\" && node --version && echo \"npx tsc --version\" && npx tsc --version && npx tsc -p ./tsconfig.build.json",
    "spec": "NODE_ENV=test jest --runInBand --forceExit",
    "console": "npx ts-node --transpile-only --project ./tsconfig.json ./test-app/app/conf/repl.ts",
    "lint": "yarn run eslint --no-warn-ignored \"src/**/*.ts\" \"spec/**/*.ts\""
  },
  "dependencies": {
    "@rvohealth/dream": "^0.5.16",
    "luxon": "^3.4.4",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/luxon": "^3.4.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^9.0.0",
    "jest": "^29.7.0",
    "luxon-jest-matchers": "^0.1.14",
    "prettier": "^3.2.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.6.0"
  }
}
