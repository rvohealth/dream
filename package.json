{
  "name": "@rvohealth/dream",
  "version": "0.1.41",
  "description": "dream orm",
  "main": "src/index.ts",
  "repository": "https://github.com/rvohealth/dream.git",
  "author": "avocadojesust",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dream": "yarn dreamts",
    "dreamcore": "APP_ROOT_PATH=$(pwd)/test-app DREAM_CORE_DEVELOPMENT=1 yarn dreamts",
    "dreamjs": "node  --experimental-modules dist/boot/cli.js",
    "dreamts": "DREAM_OMIT_DIST_FOLDER=1 TS_SAFE=1 npx ts-node -r ./src/helpers/loadEnv.ts boot/cli.ts",
    "build:package-json": "DREAM_OMIT_DIST_FOLDER=1 TS_SAFE=1 npx ts-node -r ./src/helpers/loadEnv.ts boot/adapt-package-json.ts",
    "build:boot": "rm -rf dist && npx tsc -p ./tsconfig.boot.json",
    "build": "echo \"building dream boot...\" && yarn build:boot && echo \"building dream...\" && npx tsc -p ./tsconfig.build.json && yarn build:package-json",
    "after:build": "cp ../../.dream.yml dist/.dream.yml",
    "build:boot-core": "rm -rf dist && APP_ROOT_PATH=$(pwd)/test-app npx tsc -p ./tsconfig.boot.json && APP_ROOT_PATH=$(pwd)/test-app npx tsc -p ./tsconfig.boot.json",
    "build:core": "APP_ROOT_PATH=$(pwd)/test-app yarn before:build:core && APP_ROOT_PATH=$(pwd)/test-app  npx tsc -p ./tsconfig.build.core.json && yarn after:build:core",
    "before:build:core": "rm -rf dist && mkdir dist && APP_ROOT_PATH=$(pwd)/test-app yarn build:boot-core",
    "after:build:core": "cp .dream.yml dist/.dream.yml && mkdir dist/test-app/conf/env && cp test-app/conf/env/db.yml dist/test-app/conf/env/db.yml && yarn build:package-json",
    "build:docs": "node dist/boot/build-docs.js",
    "spec": "APP_ROOT_PATH=$(pwd)/test-app DREAM_CORE_DEVELOPMENT=1 NODE_ENV=test DREAM_CORE_SPEC_RUN=1 jest --runInBand --forceExit",
    "console": "yarn dream console --core"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@types/jest": "^29.5.5",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/lodash.isempty": "^4.4.7",
    "@types/lodash.sortby": "^4.7.7",
    "@types/lodash.uniq": "^4.5.7",
    "@types/lodash.uniqwith": "^4.5.7",
    "@types/luxon": "^3.3.0",
    "@types/node": "^18.15.11",
    "@types/pg": "^8.10.2",
    "@types/pluralize": "^0.0.29",
    "commander": "^10.0.0",
    "dotenv": "^16.0.3",
    "jest": "^29.7.0",
    "kysely": "^0.26.2",
    "kysely-codegen": "^0.10.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isempty": "^4.4.0",
    "lodash.sortby": "^4.7.0",
    "lodash.uniq": "^4.5.0",
    "lodash.uniqwith": "^4.5.0",
    "luxon": "^3.3.0",
    "pg": "^8.11.3",
    "pluralize": "^8.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3",
    "yaml": "^2.2.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jest-plugin-context": "^2.9.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "bcrypt": "^5.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest-plugin-context": "^2.9.0",
    "luxon-jest-matchers": "^0.1.14",
    "typedoc": "^0.24.6"
  }
}
