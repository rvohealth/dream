{
  "name": "@rvohealth/dream",
  "version": "0.13.19",
  "description": "dream orm",
  "main": "dist/src/index.js",
  "repository": "https://github.com/rvohealth/dream.git",
  "author": "RVOHealth",
  "license": "MIT",
  "files": [
    "boilerplate/**/*",
    "dist/**/*",
    "docs/**/*"
  ],
  "bin": "./dist/src/global-cli/dream.js",
  "scripts": {
    "publish": "yarn && yarn npm publish",
    "dream": "yarn dreamts",
    "dreamjs": "node --experimental-modules dist/test-app/cli/index.js",
    "dreamts": "npx ts-node --transpile-only -r ./src/helpers/loadEnv.ts test-app/cli/index.ts",
    "dreamcore": "DREAM_CORE_DEVELOPMENT=1 yarn dreamts",
    "gdreamcore": "DREAM_CORE_DEVELOPMENT=1 npx ts-node --transpile-only ./global-cli/main.ts",
    "build": "echo \"building dream...\" && rm -rf dist && npx tsc -p ./tsconfig.build.json",
    "build:core": "rm -rf dist && mkdir dist && npx tsc -p ./tsconfig.build.core.json",
    "spec": "DREAM_CORE_DEVELOPMENT=1 NODE_ENV=test DREAM_CORE_SPEC_RUN=1 jest --maxWorkers=${DREAM_PARALLEL_TESTS:-1} --forceExit",
    "console": "DREAM_CORE_DEVELOPMENT=1 npx ts-node --transpile-only --project ./tsconfig.json ./test-app/app/conf/repl.ts",
    "types:trace": "rm -rf dist && npx tsc -p ./tsconfig.build.json --generateTrace ./typetrace --diagnostics && yarn analyze-trace ./typetrace --skipMillis 100 --forceMillis 300",
    "types:trace-test-app": "rm -rf dist && npx tsc -p ./tsconfig.trace.json --generateTrace ./typetrace --diagnostics && yarn analyze-trace ./typetrace --skipMillis 100 --forceMillis 300",
    "format": "yarn run prettier . --write",
    "lint": "yarn run eslint --no-warn-ignored \"src/**/*.ts\" \"spec/**/*.ts\" \"test-app/**/*.ts\" && yarn run prettier . --check",
    "prepack": "yarn build && yarn dreamcore build:docs"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/lodash.clonedeepwith": "^4.5.9",
    "@types/lodash.isempty": "^4.4.9",
    "@types/lodash.sortby": "^4.7.9",
    "@types/lodash.uniq": "^4.5.9",
    "@types/lodash.uniqwith": "^4.5.9",
    "@types/pluralize": "^0.0.33",
    "colorette": "^2.0.20",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "lodash.clonedeep": "^4.5.0",
    "lodash.clonedeepwith": "^4.5.0",
    "lodash.isempty": "^4.4.0",
    "lodash.sortby": "^4.7.0",
    "lodash.uniq": "^4.5.0",
    "lodash.uniqwith": "^4.5.0",
    "pluralize": "^8.0.0"
  },
  "peerDependencies": {
    "@rvohealth/dream-spec-helpers": "*",
    "@types/luxon": "^3.4.2",
    "@types/node": "*",
    "@types/pg": "*",
    "kysely": "^0.27.4",
    "kysely-codegen": "~0.17.0",
    "luxon": "^3.5.0",
    "pg": "*"
  },
  "devDependencies": {
    "@eslint/js": "=9.0.0",
    "@rvohealth/dream-spec-helpers": "^0.0.8",
    "@types/jest": "^29.5.12",
    "@types/jest-plugin-context": "^2.9.7",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.5.1",
    "@types/pg": "^8.11.8",
    "@types/winston": "^2.4.4",
    "@typescript/analyze-trace": "^0.10.1",
    "eslint": "=9.0.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "jest-plugin-context": "^2.9.0",
    "kysely": "^0.27.4",
    "kysely-codegen": "~0.17.0",
    "luxon": "^3.5.0",
    "luxon-jest-matchers": "^0.1.14",
    "pg": "^8.12.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.6",
    "typescript": "^5.5.4",
    "typescript-eslint": "=7.18.0",
    "winston": "^3.14.2"
  },
  "packageManager": "yarn@4.4.1"
}
