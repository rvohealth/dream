{
  "name": "dream",
  "version": "0.0.0",
  "description": "dream orm",
  "main": "src/index.ts",
  "repository": "https://github.com/avocadojesus/dream.git",
  "author": "avocadojesust",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dream": "node dist/boot/cli.js",
    "build-boot": "rm -rf dist && npx tsc -p ./tsconfig.boot.json",
    "build": "rm -rf dist && npx tsc -p ./tsconfig.build.json",
    "build-dev": "yarn before-dist-dev && npx tsc -p ./tsconfig.build-dev.json && yarn after-build-dev",
    "before-dist-dev": "rm -rf dist && mkdir dist",
    "after-build-dev": "cp .dream.yml dist/.dream.yml && mkdir dist/test-app/conf/env && cp test-app/conf/env/db.yml dist/test-app/conf/env/db.yml",
    "build:docs": "node dist/boot/build-docs.js",
    "spec": "yarn dream spec --core",
    "console": "yarn dream console --core",
    "postinstall": "if [ ! -e .no-dream-postinstall ]; then NODE_ENV=development yarn dream sync:existing && yarn build; fi"
  },
  "dependencies": {
    "@types/jest": "^29.5.0",
    "@types/lodash.isempty": "^4.4.7",
    "@types/lodash.sortby": "^4.7.7",
    "@types/lodash.uniq": "^4.5.7",
    "@types/lodash.uniqwith": "^4.5.7",
    "@types/luxon": "^3.3.0",
    "@types/node": "^18.15.11",
    "@types/pg": "^8.6.6",
    "@types/pluralize": "^0.0.29",
    "commander": "^10.0.0",
    "dotenv": "^16.0.3",
    "jest": "^29.5.0",
    "kysely": "^0.26.1",
    "kysely-codegen": "^0.10.0",
    "lodash.isempty": "^4.4.0",
    "lodash.sortby": "^4.7.0",
    "lodash.uniq": "^4.5.0",
    "lodash.uniqwith": "^4.5.0",
    "luxon": "^3.3.0",
    "pg": "^8.10.0",
    "pluralize": "^8.0.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jest-plugin-context": "^2.9.5",
    "bcrypt": "^5.1.0",
    "jest-plugin-context": "^2.9.0",
    "typedoc": "^0.24.6"
  }
}
