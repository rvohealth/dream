{
  "name": "@rvohealth/dream",
  "version": "0.10.0",
  "description": "dream orm",
  "main": "dist/src/index.js",
  "repository": "https://github.com/rvohealth/dream.git",
  "author": "RVOHealth",
  "license": "MIT",
  "private": false,
  "files": [
    "tsconfig.build.json",
    "tsconfig.json",
    "shared/**/*",
    "psychic-support-helpers/**/*",
    "spec-helpers/**/*",
    "src/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "dream": "yarn dreamts",
    "dreamjs": "node --experimental-modules dist/test-app/cli/index.js",
    "dreamts": "npx ts-node --transpile-only -r ./src/helpers/loadEnv.ts test-app/cli/index.ts",
    "dreamcore": "DREAM_CORE_DEVELOPMENT=1 yarn dreamts",
    "gdreamcore": "npx ts-node --transpile-only ./global-cli/main.ts",
    "build:global-cli": "npx tsc -p ./tsconfig.global-cli.json",
    "build": "echo \"building dream...\" && rm -rf dist && npx tsc -p ./tsconfig.build.json",
    "build:core": "rm -rf dist && mkdir dist && npx tsc -p ./tsconfig.build.core.json",
    "build:docs": "node dist/boot/build-docs.js",
    "spec": "DREAM_CORE_DEVELOPMENT=1 NODE_ENV=test DREAM_CORE_SPEC_RUN=1 jest --runInBand --forceExit",
    "console": "DREAM_CORE_DEVELOPMENT=1 npx ts-node --transpile-only --project ./tsconfig.json ./test-app/app/conf/repl.ts",
    "types:trace": "rm -rf dist && npx tsc -p ./tsconfig.build.json --generateTrace ./typetrace --diagnostics && yarn analyze-trace ./typetrace --skipMillis 100 --forceMillis 300",
    "types:trace-test-app": "rm -rf dist && npx tsc -p ./tsconfig.trace.json --generateTrace ./typetrace --diagnostics && yarn analyze-trace ./typetrace --skipMillis 100 --forceMillis 300",
    "format": "yarn run prettier . --write",
    "lint": "yarn run eslint --no-warn-ignored \"src/**/*.ts\" \"spec/**/*.ts\" \"psychic-support-helpers/**/*.ts\" \"spec-helpers/**/*.ts\" \"test-app/**/*.ts\" \"boot/**/*.ts\" && yarn run prettier . --check"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@types/jest": "^29.5.12",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/lodash.clonedeepwith": "^4.5.9",
    "@types/lodash.isarray": "^4.0.9",
    "@types/lodash.isempty": "^4.4.9",
    "@types/lodash.sortby": "^4.7.9",
    "@types/lodash.uniq": "^4.5.9",
    "@types/lodash.uniqwith": "^4.5.9",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.17",
    "@types/pg": "^8.11.0",
    "@types/pluralize": "^0.0.33",
    "commander": "^12.0.0",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "kysely": "^0.27.2",
    "kysely-codegen": "^0.11.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.clonedeepwith": "^4.5.0",
    "lodash.isarray": "^4.0.0",
    "lodash.isempty": "^4.4.0",
    "lodash.sortby": "^4.7.0",
    "lodash.uniq": "^4.5.0",
    "lodash.uniqwith": "^4.5.0",
    "luxon": "^3.4.4",
    "pg": "^8.11.3",
    "pluralize": "^8.0.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "yaml": "^2.3.4"
  },
  "peerDependencies": {
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jest-plugin-context": "^2.9.5",
    "@typescript/analyze-trace": "^0.10.1",
    "bcrypt": "^5.1.1",
    "eslint": "^9.0.0",
    "jest-extended": "^4.0.2",
    "jest-plugin-context": "^2.9.0",
    "luxon-jest-matchers": "^0.1.14",
    "prettier": "^3.2.5",
    "typedoc": "^0.26.6",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.6.0"
  }
}
