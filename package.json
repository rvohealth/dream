{
  "type": "module",
  "name": "@rvohealth/dream",
  "version": "0.29.0-fgbeta-4",
  "description": "dream orm",
  "repository": "https://github.com/rvohealth/dream.git",
  "author": "RVOHealth",
  "license": "MIT",
  "main": "./dist/esm/src/index.js",
  "module": "./dist/esm/src/index.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js"
    }
  },
  "files": [
    "boilerplate/**/*",
    "dist/**/*",
    "docs/**/*"
  ],
  "bin": "./dist/src/global-cli/dream.js",
  "scripts": {
    "publish": "yarn && yarn npm publish",
    "dream": "yarn dreamts",
    "dreamjs": "node --experimental-modules dist/test-app/cli/index.js",
    "dreamts": "node --experimental-specifier-resolution=node --trace-warnings --loader ts-node/esm -r ts-node/register/transpile-only ./test-app/cli/index.ts",
    "dreamcore": "DREAM_CORE_DEVELOPMENT=1 yarn dreamts",
    "gdreamcore": "DREAM_CORE_DEVELOPMENT=1 npx ts-node --transpile-only ./src/global-cli/dream.ts",
    "build": "echo \"building cjs...\" && rm -rf dist && npx tsc -p ./tsconfig.cjs.build.json && echo \"building esm...\" && npx tsc -p ./tsconfig.esm.build.json",
    "build:core": "rm -rf dist && mkdir dist && npx tsc -p ./tsconfig.cjs.build.core.json && npx tsc -p ./tsconfig.esm.build.core.json",
    "_spec": "DREAM_CORE_DEVELOPMENT=1 NODE_ENV=test DREAM_CORE_SPEC_RUN=1 yarn jest --module=ESNext --maxWorkers=${DREAM_PARALLEL_TESTS:-1} --config=jest.config.ts --forceExit",
    "__spec": "DREAM_CORE_DEVELOPMENT=1 NODE_ENV=test DREAM_CORE_SPEC_RUN=1 yarn jest --maxWorkers=${DREAM_PARALLEL_TESTS:-1} --config=jest.config.js --forceExit",
    "spec": "DREAM_CORE_DEVELOPMENT=1 NODE_ENV=test DREAM_CORE_SPEC_RUN=1 vitest --no-file-parallelism --maxWorkers=1 --minWorkers=1 --maxConcurrency=1",
    "console:cjs": "DREAM_CORE_DEVELOPMENT=1 npx ts-node --transpile-only --project ./tsconfig.cjs.json ./test-app/app/conf/repl.ts",
    "console:esm": "DREAM_CORE_DEVELOPMENT=1 npx ts-node --transpile-only --project ./tsconfig.json ./test-app/app/conf/repl.ts",
    "types:cjs:trace": "rm -rf dist && npx tsc -p ./tsconfig.cjs.build.json --generateTrace ./typetrace --diagnostics && yarn analyze-trace ./typetrace --skipMillis 100 --forceMillis 300",
    "types:esm:trace": "rm -rf dist && npx tsc -p ./tsconfig.esm.build.json --generateTrace ./typetrace --diagnostics && yarn analyze-trace ./typetrace --skipMillis 100 --forceMillis 300",
    "types:cjs:trace-test-app": "rm -rf dist && npx tsc -p ./tsconfig.cjs.trace.json --generateTrace ./typetrace --diagnostics && yarn analyze-trace ./typetrace --skipMillis 100 --forceMillis 300",
    "types:esm:trace-test-app": "rm -rf dist && npx tsc -p ./tsconfig.esm.trace.json --generateTrace ./typetrace --diagnostics && yarn analyze-trace ./typetrace --skipMillis 100 --forceMillis 300",
    "format": "yarn run prettier . --write",
    "lint": "yarn run eslint --no-warn-ignored \"src/**/*.ts\" \"spec/**/*.ts\" \"test-app/**/*.ts\" && yarn run prettier . --check",
    "prepack": "yarn build && yarn dreamcore build:docs"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "colorette": "^2.0.20",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "pluralize-esm": "^9.0.5"
  },
  "peerDependencies": {
    "@rvohealth/dream-spec-helpers": "*",
    "@types/luxon": "^3.4.2",
    "@types/node": "*",
    "@types/pg": "*",
    "kysely": "^0.27.4",
    "kysely-codegen": "~0.17.0",
    "luxon": "^3.5.0",
    "pg": "*"
  },
  "devDependencies": {
    "@eslint/js": "=9.0.0",
    "@rvohealth/dream-spec-helpers": "=0.1.0-fgbeta-15",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.5.1",
    "@types/pg": "^8.11.8",
    "@types/winston": "^2.4.4",
    "@typescript/analyze-trace": "^0.10.1",
    "eslint": "=9.0.0",
    "jest-extended": "^4.0.2",
    "kysely": "^0.27.4",
    "kysely-codegen": "~0.17.0",
    "luxon": "^3.5.0",
    "luxon-jest-matchers": "^0.1.14",
    "pg": "^8.12.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.6",
    "typescript": "^5.8.2",
    "typescript-eslint": "=7.18.0",
    "vitest": "^3.0.8",
    "winston": "^3.14.2"
  },
  "packageManager": "yarn@4.4.1"
}
